{
  "id": "blogpost",
  "label": "BlogPost",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "author": {
        "type": "Text",
        "config": {
          "label": "Author",
          "placeholder": ""
        }
      },
      "publicationdate": {
        "type": "Date",
        "config": {
          "label": "PublicationDate",
          "placeholder": ""
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "strong,em,paragraph"
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "slug",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "blog_image": {
              "type": "SharedSlice"
            },
            "text_block": {
              "type": "SharedSlice"
            },
            "video_block": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}